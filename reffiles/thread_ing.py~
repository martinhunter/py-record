import threading
import time


def fun1():
    for i in range(4):
        print("func1 running\n")
        time.sleep(1)
        
def fun2(ras):
    for i in range(ras):
        print("func2 running\n")
        time.sleep(1)
        
def main():
    t1 = threading.Thread(target=fun1)
    t2 = threading.Thread(target=fun2, args=(6,))
    t1.start()
    t2.start()

main()
